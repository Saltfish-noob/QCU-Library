        <!-- 内容头部 -->
        <section class="content-header">
            <h1>
                书籍管理 <small>书籍表单</small>
            </h1>
            <ol class="breadcrumb">
                <li><a href="${pageContext.request.contextPath}/pages/back-end/main.jsp"><i
                        class="fa fa-dashboard"></i> 首页</a></li>
                <li><a
                        href="${pageContext.request.contextPath}/user/findAll.do">书籍管理</a></li>
                <li class="active">书籍表单</li>
            </ol>
        </section>
        <!-- 内容头部 /-->

        <form action="${pageContext.request.contextPath}/book/addBook.do" method="post" enctype="multipart/form-data">
            <section class="content"> <!-- .box-body -->
                <div class="box box-primary">
                    <div class="box-header with-border">
                        <h3 class="box-title">基本信息</h3>
                    </div>
                    <div class="box-body">

                        <!--工具栏-->

                        <!--工具栏/-->
                        <!-- 数据表格 -->
                        <div class="table-box">

                            <!--数据列表/-->
                        </div>
                        <div class="panel panel-default">
                            <div class="panel-heading">书籍信息</div>
                            <div class="row data-type">
                                <div class="col-md-2 title">书名</div>
                                <div class="col-md-4 data">
                                    <input required type="text" class="form-control" name="bookName"
                                           placeholder="书名" >
                                </div>
                                <div class="col-md-2 title">作者</div>
                                <div class="col-md-4 data">
                                    <input required type="text" class="form-control" name="author"
                                           placeholder="作者">
                                </div>
                                <div class="col-md-2 title">类别</div>
                                <div class="col-md-4 data">
                                    <select class="form-control select2" style="width: 100%"
                                            name="category">
                                        <c:forEach items="${categoryList}" var="category">
                                            <option value="${category.id}" >${category.categoryName}</option>
                                        </c:forEach>
                                    </select>
                                </div>
                                <div class="col-md-2 title">出版社</div>
                                <div class="col-md-4 data">
                                    <input required type="text" class="form-control" name="press"
                                           placeholder="出版社" >
                                </div>
                                <div class="col-md-2 title">状态</div>
                                <div class="col-md-4 data">
                                    <select class="form-control select2" style="width: 100%"
                                            name="status">
                                        <option value="0" >被借阅</option>
                                        <option value="1" selected>在馆</option>
                                    </select>
                                </div>
                                <div class="col-md-2 title">ISBN</div>
                                <div class="col-md-4 data">
                                    <input required type="text" class="form-control" name="ISBN"
                                           placeholder="ISBN">
                                </div>
                                <div class="col-md-2 title">书籍图片</div>
                                <div class="col-md-4">
                                    <input type="file" class="form-control" name="file">
                                </div>
                                <div class="col-md-2 title">本数</div>
                                <div class="col-md-4">
                                    <input required value="1" type="number" class="form-control" name="number" onkeyup="value=value.replace(/[^\d]/g,'').replace(/^0{1,}/g,'')">
                                </div>
                            </div>
                        </div>
                        <!--数据列表-->
                        <!-- 数据表格 /-->

                    </div>
                    <!-- /.box-body -->

                </div>
                <!--订单信息/--> <!--工具栏-->
                <div class="box-tools text-center">
                    <button type="submit" class="btn bg-maroon">保存</button>
                    <button type="button" class="btn bg-default"
                            onclick="history.back(-1);">返回
                    </button>
                </div>
